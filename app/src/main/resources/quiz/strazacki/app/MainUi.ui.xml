<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.button {
			width: 100px;
			height: 100px;
		}
	</ui:style>
	<g:Grid width="500px" height="500px">
		<g:row>
			<g:customCell>
				<g:Image ui:field="img" />
			</g:customCell>
		</g:row>
		<g:row>
			<g:customCell>
				<g:Grid width="100%">
					<g:row>
						<g:customCell>
							<g:Button styleName='{style.button}' ui:field="a">A</g:Button>
						</g:customCell>
						<g:customCell>
							<g:Button styleName='{style.button}' ui:field="b">B</g:Button>
						</g:customCell>
						<g:customCell>
							<g:Button styleName='{style.button}' ui:field="c">C</g:Button>
						</g:customCell>
						<g:customCell>
							<g:Button styleName='{style.button}' ui:field="d">D</g:Button>
						</g:customCell>
						<g:customCell>
							<g:Button styleName='{style.button}' ui:field="e">E</g:Button>
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:customCell>
		</g:row>
	</g:Grid>
</ui:UiBinder> 